    /* Modal styles */
/* Modal overlay styling */
/* Modal overlay styling */
#cropperModal {
    display: none; /* Hidden by default */
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.6); /* Slightly darker semi-transparent background */
    justify-content: center;
    align-items: center;
    z-index: 9999; /* Ensure the modal is above other elements */
}

/* Modal content styling, centered and limited to 80% of viewport height */
.modal-content {
    background-color: #fff;
    padding: 20px;
    border-radius: 8px;
    text-align: center;
    width: 50%; /* Width is 50% of viewport */
    height: 80vh; /* Height is 80% of viewport */
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2); /* Adds a shadow for better contrast */
}

/* Image container styling */
.image-container {
    flex-grow: 1; /* Allows image container to take up available space */
    display: flex;
    justify-content: center;
    align-items: center;
    overflow: hidden;
    margin-bottom: 15px; /* Adds space between the image and buttons */
}

#imageToCrop {
    max-width: 100%;
    max-height: 100%; /* Ensures image stays within the container */
    border-radius: 5px;
}

/* Button container styling */
.button-container {
    display: flex;
    gap: 10px; /* Adds space between buttons */
}

/* Button styling */
button {
    padding: 10px 20px;
    background-color: #faea5a;
    color: white;
    border: none;
    cursor: pointer;
    border-radius: 5px;
    font-size: 16px;
    transition: background-color 0.3s;
}

/* Hover effect for all buttons */
button:hover {
    background-color: #fbb710;
}

/* Close button specific styling */
#closeModalButton {
    background-color: #4A4A4A
}

#closeModalButton:hover {
    background-color: rgb(109, 107, 107);
}


